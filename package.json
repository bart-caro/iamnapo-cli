{
	"name": "iamnapo",
	"version": "1.3.0",
	"description": "The Napoleon Oikonomou CLI",
	"keywords": [
		"cli",
		"cli-app",
		"iamnapo",
		"npx",
		"personal"
	],
	"homepage": "https://github.com/iamnapo/iamnapo-cli#readme",
	"bugs": {
		"url": "https://github.com/iamnapo/iamnapo-cli/issues"
	},
	"repository": "github:iamnapo/iamnapo-cli",
	"license": "MIT",
	"author": {
		"name": "Napoleon Oikonomou",
		"email": "Napoleonoikon@gmail.com",
		"url": "https://iamnapo.me"
	},
	"type": "module",
	"bin": {
		"iamnapo": "./dist/cli.js"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "esbuild source/cli.jsx --bundle --external:./node_modules/* --platform=node --format=esm --outfile=dist/cli.js",
		"prepare": "npm run build",
		"start": "node dist/cli.js",
		"test": "eslint . --ext .jsx,.js --cache"
	},
	"eslintConfig": {
		"extends": "iamnapo/react",
		"ignorePatterns": "dist"
	},
	"dependencies": {
		"chalk": "^5.0.1",
		"ink": "^3.2.0",
		"ink-select-input": "^4.2.1",
		"meow": "^10.1.2",
		"open": "^8.4.0",
		"react": "^18.1.0",
		"terminal-link": "^3.0.0"
	},
	"devDependencies": {
		"esbuild": "^0.14.39",
		"eslint": "^8.14.0",
		"eslint-config-iamnapo": "^24.1.0"
	},
	"engines": {
		"node": ">=16"
	}
}
